    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rntodolistapp.todolistapp"
    },
    "extra": {
      "eas": {
        "projectId": "08c2a70f-83e0-4ee9-84fe-012084f25dad"
      }
    }
  }
